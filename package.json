				"category": "Maven",
				"command": "maven.dependency.goToEffective",
				"title": "%contributes.commands.maven.dependency.goToEffective%"
			},
			{
				"category": "Maven",
				"command": "maven.project.goToDefinition",
				"title": "%contributes.commands.maven.project.goToDefinition%"
