			},
			{
				"category": "Maven",
				"command": "maven.project.addDependency",
				"icon": "$(add)",
				"title": "%contributes.commands.maven.project.addDependency%"
			},
			{
				"category": "Maven",
				"command": "maven.project.showDependencies",
				"title": "%contributes.commands.maven.project.showDependencies%"
