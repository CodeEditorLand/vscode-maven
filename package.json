{
	"name": "vscode-maven",
	"displayName": "Maven for Java",
	"version": "0.0.1",
	"private": false,
	"description": "%description%",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./dist/extension",
	"scripts": {
		"build-plugin": "node scripts/build-jdtls-ext.js",
		"compile": "tsc -p ./",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"tslint": "eslint .",
		"update-tpn": "node scripts/update-third-party-notice.js",
		"update-xsd": "tsc scripts/generate-maven-xsd.ts && node scripts/generate-maven-xsd.js",
		"vscode:prepublish": "webpack --mode production",
		"watch": "webpack --mode development --watch --progress",
		"webpack": "webpack --mode development"
	},
