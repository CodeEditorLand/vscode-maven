				},
				{
					"command": "maven.goal.compile",
					"when": "never"
				},
				{
					"command": "maven.goal.test",
					"when": "never"
				},
				{
					"command": "maven.goal.test-compile",
					"when": "never"
				},
				{
					"command": "maven.goal.package",
					"when": "never"
				},
				{
					"command": "maven.goal.verify",
					"when": "never"
				},
				{
					"command": "maven.goal.install",
					"when": "never"
				},
				{
					"command": "maven.goal.site",
					"when": "never"
				},
